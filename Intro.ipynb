    "# y= mx + c: y(output) = m(weight) . x(input) + c(Bias)\n",
    "#linear_regression\n",
    "def linear(x):\n",
    "    m = 0.5\n",
    "    c = 2\n",
    "    y = m * float(x) + c\n",
    "    return y\n"
